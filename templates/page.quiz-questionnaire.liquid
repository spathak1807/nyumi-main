  allCheckboxes.on("click", function() {
    for (var i=0 ; i<allCheckboxes.length ; i++) {
      if (allCheckboxes[i].checked == true) {
         noneOfTheAbove.prop("disabled", true) ;
         break; 
      }
      else {
      // enable none of the above checkbox
      noneOfTheAbove.prop("disabled", false);
    }
    }
   
  });
});


