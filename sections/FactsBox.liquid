  {
    "name": "FactsBox",
    "settings": [
      {
      "type": "text",
      "id": "heading",
      "label": "heading"
    }
    ],
    	"blocks": [
     {
       "name": "Slide",
       "type": "slide",
       "settings": [
         {
           "type": "text",
           "id": "title",
           "label": "Slide_title"
         } ,
         {
           "type": "text",
           "id": "description",
           "label": "Slide_description"
         }
       ]
     }
  ] ,
 "presets": [
    {
      "name": "FactsBox",
