<script>
  $('.expert-btn').click(function(event){
    event.preventDefault()
    dataLayer.push({"event":"expert-consult"})
    window.location.href = '/pages/schedule-appointment'
  })
  $('.expert-close').click(function(event) {
    event.preventDefault()
    $('.close-popup').css('display','flex')
    $('body').css('overflow','hidden')
  })
  $('button.resume').click(function(event){
    event.preventDefault()
    $('.close-popup').hide()
    $('body').css('overflow-y','scroll')
  })
  $('button.quit').click(function(event){
    event.preventDefault()
    $('.close-popup').hide()
    dataLayer.push({"event":"expert-consult-close"})
    $('body').css('overflow-y','scroll')
    window.location.href = '/collections/all'
  })
  $('button.quiz-btn').click(function(event){
    event.preventDefault()
    dataLayer.push({"event":"start-quiz"})
    window.location.href = '/pages/quiz-and-questionnaire'
  })
</script>

