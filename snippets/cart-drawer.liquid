<style>
/* Required Menu CSS */
.js-menu__context {
  background: rgba(0,0,0, 0.5);
  left: 0;
  opacity: 0;
  position: fixed;
  top: 0;
  transition: opacity 0.5s, visibility 0.5s;
  visibility: hidden;
  width: 100%;
  height: 100%;
  z-index: 999999;
}
    
.js-menu { 
  position: absolute;
  height: 100%;
  width: 400px;
  max-width: 85vw;
  margin: 0;
  background: #fff;
  z-index: 99;
  overflow-y: auto;

  -webkit-overflow-scrolling: touch;
  -ms-overflow-style: -ms-autohiding-scrollbar;
  transition: background 0.5s, transform 0.5s;
}

.js-menu--left {
    -webkit-transform: translateX(-100%);
    transform: translateX(-100%);
}
.js-menu--right {
    -webkit-transform: translateX(100%);
    transform: translateX(100%);
    position: absolute;
    right: 0;
}
.js-menu__open {
  cursor: pointer;
}

.js-menu__close {
  cursor: pointer;
}

.js-menu__expanded {
  -webkit-transform: translateX(0);
  transform: translateX(0);
  visibility: visible;
  opacity: 1;
  overflow-x:hidden;
}

/* Extra CSS */  
.drawer_header{  
  background: #fff4ef;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  width: 400px;
  height: 60px;
  z-index: 1;
}
.drawer_header_heading {
    padding: 10px 20px 23px;
    line-height: 40px;
    font-weight: 500;
    color: #231f20;
    font-family: corbenr;
    outline: none;
    font-size: 24px;
}
.js-menu__close{
    cursor: pointer;
    width: 16px;
    margin-right: 25px;
}
.drawer_product_detail{
    margin: 15px 15px;
/*     height: 400px; */
    height: 380px;
    overflow-y: auto;
}
.drawer_product_detail .minicart_item{
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #cacaca;
    padding-top:15px;
} 
.drawer_product_detail .minicart_item:first-child {
    padding-top: 0;
}
.drawer_product_detail .minicart_item_img {
    width: 65px;
}
.drawer_product_detail .minicart_item_title{
    display: flex;
    flex-wrap: wrap;
    width: calc(100% - 110px);
}
.drawer_product_detail .minicart_title{
  line-height: 24px;
  font-weight: 500;
  font-size: 17px;
  color: #231f20;
  font-family: corbenr;
  text-transform: lowercase;
  width: 100%;
}
.drawer_product_detail .minicart_item_price{
    font-family: manropes;
    font-weight: 500;
    font-size: 15px;
    color: #231f20;
    width: 50%;
    margin: 8px 0 0;
}
.drawer_remove_and_qty{
    margin-top: 7px;
    margin-bottom: 15px;
    width: 43%;
    display: flex;
    align-items: center;
}    
.drawer_remove_and_qty .minicart_item_qty{  
    display: inline-flex;
    align-items: center;
    border: 1px solid #939393;
    border-radius: 20px;
}
.drawer_remove_and_qty .minicart_item_qty .minius_sign {
    width: 30px;
    height: 30px;
    cursor: pointer;
    position: relative;
}
.drawer_remove_and_qty .minicart_item_qty .plus_sign {
    width: 30px;
    height: 30px;
    cursor: pointer;
    position: relative;
}
.drawer_remove_and_qty .minicart_item_qty .minicart_qty{
   padding: 3px 7px;
   font-family: manropes;
   font-weight: 500;
   font-size: 13px;
   color: #231f20;
}
.drawer_remove_and_qty .minicart_item_qty .plus_sign:before{
    content: "";
    background-image: url('https://cdn.shopify.com/s/files/1/0412/3027/0631/t/73/assets/plus.png');
    position: absolute;
    width: 9px;
    height: 9px;
    top: 50%;
    left: 50%;
    -moz-transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain;
}
.drawer_remove_and_qty .minicart_item_qty .minius_sign:before{
    content: "";
    background-image: url('https://cdn.shopify.com/s/files/1/0412/3027/0631/t/73/assets/minius.png');
    position: absolute;
    width: 9px;
    height: 9px;
    top: 50%;
    left: 50%;
    -moz-transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain;
}
.minicart_footer{ 
    padding: 15px;
    padding-bottom: 0;
    position: relative;
    bottom: 0;
    width: 100%;
}
.minicart_footer .cart__shipping{
    font-family: manroper;
    font-size: 14px;
    color: #9b9b9b;
    margin-bottom: 5px;
    display: flex;
    justify-content: space-between;
    padding: 5px 10px;
    margin-top: 10px;
}
.minicart_footer .cart__shipping span{
    font-family: manroper;
    font-size: 14px;
    color: #9b9b9b;  
}
.minicart_subtotal{
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    padding-bottom: 20px;
    flex-wrap: wrap;
    padding-left: 10px;
    padding-right:10px;
    margin-bottom: 0!important;
}
.minicart_checkout_btn{
color: #fff;
    border-radius: 0px;
    font-family: manropes;
    font-size: 20px;
    padding: 10px;
    background: #f47169;
    letter-spacing: .5px;
    text-align: center;
    z-index: 9999;
    bottom: 0;
    position: absolute;
    position: sticky;
    top: 0;
    width: 100%;
    max-width: 100%;
    display: block;
}
.quantity-selector.quantity-input{ 
    width: 50%;
    padding: 0;
    margin: 0;
    border: none;
    text-align: center;
}
.bundle_div_outer{
    column-gap: 5px;
    background-color: #ed716b42;
    align-items: center;
    padding: 10px;
    height: 190px;
    min-width: 140px;
    flex: 0 0 140px;
/*     margin-bottom: 10px; */
    margin-right: 12px;
    position: relative;
    text-align: center;
    border-radius: 8px;
}
.min_cart_drawer_inner .product-details__item--variant-option{
   font-size:13px;
   display:block;
   width:100%;
}
.drawer_viewcart_btn{
     width: 100%;
    text-align: center;
    display: block;
    color: #ed716b;
    margin-bottom:15px;
}
.drawer_viewcart_btn:hover{
    text-decoration:underline;    
}
.minicart_footer h2{
      font-family: 'corbenr';
    text-transform: inherit;
    font-weight: 500;
    color: #231f20;
    font-size: 20px;
} 
  .bundle_div_outer .bundle_product_title_para {
    line-height: 19px;
    font-weight: 500;
    font-size: 16px;
    color: #231f20;
    font-family: corbenr;
    width: 100%;
    margin-bottom: 10px;
    text-transform: capitalize;
  }
.bundle_div_outer .bundle_add_to_cart_btn { 
    display: block;
    color: #fff;
    border-radius: 0px;
    font-family: manropes;
    font-size: 30px;
    line-height: 17px;
    padding: 10px;
    margin-bottom: 15px;
    background: #f47169;
    letter-spacing: .5px;
    text-align: center;
    text-transform: capitalize;
    border: none;
    position: absolute;
    top: 0;
    right: 0;
    border-top-right-radius: 8px;
    border-bottom-left-radius: 8px;
}
.bundle_div_outer .bundle_product_price p{
    line-height: 24px;
    font-weight: 500;
    font-size: 17px;
    color: #231f20;
    font-family: corbenr;
    text-transform: lowercase;
    width: 100%;
}
  .minicart_footer .slick-next:before{
    display:none;
  }
  .minicart_footer .slick-prev:before{
    display:none;
  }
  .minicart_footer .slick-dots li{ 
    border: 1px solid #f47e75;
    height: 10px;
    width: 10px;
  }
  .minicart_footer .slick-dots li.slick-active {
    background: #f47e75;
  }
.minicart_footer .slick-dots li button{
    width: 10px;
    height: 10px;
}
.minicart_footer .slick-dots li button:before{
    width: 10px;
    height: 10px;
}
.upsells-stacked1{
    overflow: hidden;
    overflow-x: auto;
    flex-wrap: nowrap;
    display: flex;
} 
.upsell bundle_div_outer{
  position:relative;
}  
 .bundle_prod_variant {
   margin-bottom:0px;
 }
  .bundle_div_outer .bundle_product_title{
  text-align:left;
  }
  .bundle_div_outer .bundle_product_image img{
    width:80%;
  }
  .bundle_div_outer .bundle_product_image {
    height:100px;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .min_cart_drawer_inner{
    max-height: calc(100vh - 0px);
  }
