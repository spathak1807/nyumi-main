<div class="js-menu__context new_custom_cart_drawer">
  <div id="main-nav" class="js-menu js-menu--right">
<!--     <span class="js-menu__close">âœ•</span> -->


      <div class="min_cart_drawer_inner">
    <div class="drawer_header">
      <div class="drawer_header_heading">your cart</div>
      <div class="js-menu__close"><img src="{{ 'close.png' | asset_url }}" alt="close"/></div>
    </div>
    <div class="cart_body_wrapper">
      <div class="cart_drawer_loder hide">
        <img src="https://cdn.shopify.com/s/files/1/0412/3027/0631/t/58/assets/200w.gif">
      </div>
      {% if settings.upsell_message != blank %}
      <div class="cart_upper">
        <p>{{ settings.upsell_message }}</p>
      </div>
      {% endif %}   
      <div class="drawer_product_detail">  
        {% for item in cart.items %} 
        <div class="minicart_item" data-line="{{ item.index }}" data-id="{{ item.id}}" data-price="{{ item.price}}" data-productId ="{{ item.variant.id }}" >
          <div class="minicart_item_img"> 
            {% if item.image != null %} 
            <img src="{{ item | img_url: '120x120' }}"  alt="{{ item.image.alt | escape }}"/>
            {% endif %}
          </div>

          <div class="minicart_item_title">
            <a href="{{ item.url }}" class="minicart_title 1">{{ item.product.title }}</a>
            {%- assign variant_options = 'template ' | split: ' ' -%}
            {%- if item.product.has_only_default_variant != true -%}
            {%- assign variant_options = item.options_with_values -%}
            {%- endif -%}
            {%- assign property_size = item.properties | size -%}
            {%- for option in variant_options -%}
            <p class="product-details__item product-details__item--variant-option{% if item.product.has_only_default_variant %} hide{% endif %}" data-cart-item-option>{{ option.value }}</p>
            {%- endfor -%}
            <div class="minicart_item_price"> 
          {{ item.price | money }}
          {% comment %}
                {% if item.variant.compare_at_price > item.variant.price %}
                  <s>{{ item.variant.compare_at_price | money }}</s>
                {% endif %}
          {% endcomment %} 
            </div>

            <div class="drawer_remove_and_qty">
                <div class="qtybox minicart_item_qty">
                  <div class="minicart_sign minius_sign btnqty qtyminus icon icon-minus" data-variant-id="{{ item.variant_id }}"></div>
                  <input type="text" id="quantity" name="quantity" value="{{ item.quantity }}" min="0" class="quantity-selector quantity-input" readonly="">
                  <div class="minicart_qty_sign plus_sign btnqty qtyplus icon icon-plus" data-variant-id="{{ item.variant_id }}"></div>
                </div>

              <div class="minicart_item_remove hide">remove </div>
            </div>

          </div>
        </div>
        {% endfor %}
      </div>
    </div>

            <div class="minicart_footer">

              <!--[Upsell product]-->
                  <h2>People also bought</h2>

  <div class="upsells-stacked1">
    {% assign upsell_products = settings.product_bundle_collection %}
    {% assign upsell_count = 0 %}
      {% for product in collections[upsell_products].products %}
      
      {% assign upsell_count = upsell_count | plus: 1 %}
      {% if upsell_count <= 4 %}
      
             <div class="upsell bundle_div_outer" data-bundulProId ="{{ product.variants.first.id }}">
          
          <div class="bundle_product_image">
            <img src="{{ product | img_url: 'x100' }}" />
          </div>
          <div class="bundle_product_title">
            <p class="bundle_product_title_para">  {{ product.title }}</p>
  
            {% for option in product.options %}
            {% if option == 'Choose Pack' %}
            <p class="bundle_prod_variant">{{ product.variants.first.options[forloop.index0] }} </p>
            {% endif %}
            {% endfor %}
            
            <p class="bundle_product_price_new"> {{ product.price | money }}</p>
             
        </div>
          

      </div>
      {% endif %}
       {% endfor %}
  </div>

            
              <!--[Upsell product]-->
              
      {% if settings.free_shipping_text != blank %}
      <h4 class="cart__shipping rte text-left"><span>Shipping</span><span>{{ settings.free_shipping_text }}</span></h4>
      {% endif %}
      <div class="minicart_footer_wrap">
       <div class="minicart_subtotal">
         <span class="minicart_subtotal__title">{{ 'cart.general.subtotal' | t }}</span>
         <span class="minicart_subtotal__price">{{ cart.total_price | money_with_currency }}</span>
      </div>
      </div>
       
<!--       <a href="/checkout" class="minicart_checkout_btn">{{ 'cart.general.checkout' | t }}</a> -->
<!--       <a href="/cart" class="drawer_viewcart_btn">View Cart </a> -->
    </div>

          
         <a href="/checkout" class="minicart_checkout_btn">{{ 'cart.general.checkout' | t }}</a> 
      </div>
        
  
  </div>
   
</div>
